#include <stdio.h>

int main() {
    int largest_palindrome = 0;

    for (int i = 100; i < 1000; i++) {
        for (int j = i; j < 1000; j++) {
            int product = i * j;
            char product_str[7];
            sprintf(product_str, "%d", product);
            int len = strlen(product_str);
            int is_palindrome = 1;
            for (int k = 0; k < len / 2; k++) {
                if (product_str[k] != product_str[len - k - 1]) {
                    is_palindrome = 0;
                    break;
                }
            }
            if (is_palindrome && product > largest_palindrome) {
                largest_palindrome = product;
            }
        }
    }

    FILE *f = fopen("102-result", "w");
    fprintf(f, "%d", largest_palindrome);
    fclose(f);

    return 0;
}
